{
    "json.format.keepLines": false,
    "git.inputValidationLength": 100,
    "git.inputValidationSubjectLength": null,
    "ruff.trace.server": "messages",
    "rewrap.wholeComment": false,
    "rewrap.wrappingColumn": 100,
    "conventionalCommits.autoCommit": false,
    "conventionalCommits.promptScopes": false,
    "conventionalCommits.showNewVersionNotes": false,
    "[python]": {
        "editor.defaultFormatter": "charliermarsh.ruff"
    },
    "python.defaultInterpreterPath": "./.venv/bin/python",
    "python.analysis.diagnosticMode": "workspace",
    "python.languageServer": "Pylance",
    "python.terminal.activateEnvInCurrentTerminal": true,
    "python.testing.pytestArgs": [
        "tests"
    ],
    "python.testing.unittestEnabled": false,
    "python.testing.pytestEnabled": true,
    "python.autoComplete.extraPaths": [
        "tests"
    ],
    "autoDocstring.docstringFormat": "sphinx",
    "dbmlERDPreviewer.preferredTheme": "dark",
    "[sql]": {
        "editor.defaultFormatter": "ReneSaarsoo.sql-formatter-vsc"
    },
    "SQL-Formatter-VSCode.dialect": "postgresql",
    "SQL-Formatter-VSCode.tabSizeOverride": 4,
    "SQL-Formatter-VSCode.dataTypeCase": "lower",
    "SQL-Formatter-VSCode.expressionWidth": 80,
    "SQL-Formatter-VSCode.functionCase": "lower",
    "SQL-Formatter-VSCode.keywordCase": "upper",
    "SQL-Formatter-VSCode.newlineBeforeSemicolon": true,
    "SQL-Formatter-VSCode.paramTypes": {
        // See https://github.com/sql-formatter-org/sql-formatter/blob/master/docs/paramTypes.md for
        // documentation on this setting.
        // To allow for ":name" syntax in placeholders (used by SQLAlchemy):
        "named": [
            ":"
        ],
        // To allow for "{name}" and "%(name)s" syntax in placeholders (used by psycopg):
        "custom": [
            {
                "regex": "(\\{[a-zA-Z0-9_]+\\}|%\\([a-zA-Z0-9_]+\\)s)"
            }
        ]
    },
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "python.analysis.extraPaths": [
        "tests"
    ],
    "commentAnchors.workspace.excludeFiles": "**/{.venv,node_modules,.git,.idea,target,out,build,bin,obj,dist,vendor}/**/*",
}
